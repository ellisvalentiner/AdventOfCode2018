
function problem1(datadir::String=joinpath(@__DIR__, "data"))
    instructions = readlines(normpath(joinpath(datadir, "1.txt")))
    changes = Meta.parse.(instructions)
    partone = last(cumsum(changes))
    @info "part one $partone"
    frequencies = [0]
    i = 1
    n = length(changes)
    notfound = true
    cnt = 0
    while notfound
        frequency = last(frequencies) + changes[i]
        if frequency in frequencies
            global parttwo = frequency
            notfound = false
            break
        end
        append!(frequencies, [frequency])
        if i < n
            i += 1
        else
            cnt += 1
            @info "pass $cnt"
            i = 1
        end
    end
    return Dict("Part One" => partone, "Part Two" => parttwo)
end

problem1()

